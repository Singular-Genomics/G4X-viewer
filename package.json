{
  "name": "g4x-viewer",
  "private": true,
  "version": "3.0.1",
  "type": "module",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write --list-different",
      "eslint --fix --cache"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write --list-different"
    ],
    "src/**/*.{ts,tsx}": [
      "bash -c 'tsc --noEmit'"
    ]
  },
  "dependencies": {
    "@deck.gl-community/editable-layers": "9.1.1",
    "@deck.gl-community/layers": "9.1.1",
    "@deck.gl/core": "9.1.14",
    "@deck.gl/extensions": "9.1.14",
    "@deck.gl/geo-layers": "9.1.14",
    "@deck.gl/layers": "9.1.14",
    "@deck.gl/mesh-layers": "9.1.14",
    "@deck.gl/react": "9.1.14",
    "@deck.gl/widgets": "9.1.14",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hms-dbmi/viv": "0.18.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/x-data-grid": "^7.29.12",
    "@types/lodash": "^4.17.0",
    "@types/react-grid-layout": "^1.3.5",
    "i18next": "^25.7.2",
    "i18next-http-backend": "^3.0.2",
    "it-pipe": "^3.0.1",
    "it-tar": "^6.0.5",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "notistack": "^3.0.2",
    "plotly.js": "^3.3.0",
    "protobufjs": "^7.5.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-dropzone": "^14.3.8",
    "react-grid-layout": "^1.5.3",
    "react-i18next": "^16.4.0",
    "react-plotly.js": "^2.6.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.1",
    "@types/plotly.js": "^3.0.8",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-plotly.js": "^2.6.3",
    "@vitejs/plugin-react": "^5.1.2",
    "buffer": "^6.0.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.2.7"
  }
}
